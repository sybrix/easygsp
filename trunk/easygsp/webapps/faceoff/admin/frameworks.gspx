<html>
        <head>
          <title>Framework Faceoff - Admin</title>
        </head>
        <body>

                <%@ include file="menu.gsp" %>

                <div>
                        <a href="framework.gspx" >New Framework</a>
                </div>
                <table border="1" align="center">
                        <tr>
                            <td>Framework</td>
                            <td>Version</td>
                            <td>Url</td>
                            <td>Category</td>
                            <td>Edit</td>
                        </tr>
                        <% frameworks.each { %>
                        <tr>
                                <td>$it.framework</td>
                                <td>$it.version</td>
                                <td>$it.url</td>
                                <td>$it.category</td>
                                <td><a href="framework.gspx?frameworkId=${it.frameworkId}">Edit</a></td>
                        </tr>
                        <% } %>
                </table>

        </body>
</html>