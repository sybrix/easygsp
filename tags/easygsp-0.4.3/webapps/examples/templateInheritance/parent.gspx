                                </td>
                                <td  align="center">
                                <%@ block center %>                                         
                                <%@ endblock %>
                                </td>
                                <td  width="20%">Column 3</td>
                        </tr>
                        <tr>
                                <td colspan="3">Footer</td>
                        </tr>
                </table>

        </body>
</html>  
        