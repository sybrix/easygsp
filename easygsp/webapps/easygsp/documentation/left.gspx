<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
        "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
        <title></title>
        <link rel="stylesheet" href="style.css" type="text/css" media="screen"/>
</head>
<body>
         <div id="menu">

                 <%
                        def menu
                        def  x=0
                         request.menuItems.each {
                                if (menu != it.menu) {
                                        if (x>0){
                                               println "</ul>"
                                        }

                 %>
                                        $it.menu
                 <%
                                        println '<ul  class="menuItem">'

                                 }  %>
                                                <li><a href="content.gspx?name_id=$it.nameId" target="right">$it.menuItem</a></li>

                 <%             menu = it.menu
                                x++
                        }

                         if (x>0){
                                println "</ul>"
                         }

                %>
                 
         </div>
<%@ include file="/google_analytics.gsp" %>
</body>
</html>